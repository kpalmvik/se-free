{
  "name": "se-free",
  "version": "1.0.0",
  "description": "Wraps the Domain Availability Service for Swedish .SE domains in a promised base interface. This makes it easy to check if a specified .SE domain is available for registration, already registered, or invalid.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha tests/index.test.js -w --compilers js:babel/register",
    "test:single": "mocha tests/index.test.js --compilers js:babel/register",
    "lint": "eslint src/*.js",
    "lint-test": "npm run lint && npm run test:single",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel src/index.js -o dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kpalmvik/se-free.git"
  },
  "author": "Kristofer Palmvik <kristofer@palmvik.se> (http://kristofer.palmvik.se/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kpalmvik/se-free/issues"
  },
  "homepage": "https://github.com/kpalmvik/se-free",
  "devDependencies": {
    "babel": "5.8.29",
    "chai": "3.4.0",
    "chai-as-promised": "5.1.0",
    "eslint": "6.4.0",
    "ghooks": "0.3.2",
    "mocha": "2.3.3",
    "nock": "2.15.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint-test"
    }
  },
  "dependencies": {
    "idna-uts46": "1.0.1",
    "request-promise": "1.0.2"
  }
}
